<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RectangleView">
        <attr name="modle" format="enum">
            <enum name="top" value="0"/>
            <enum name="center" value="1"/>
            <enum name="bottom" value="2"/>
            <enum name="single" value="3"/>
        </attr>
        <attr name="left_icon" format="reference"/>
        <attr name="left_icon_visibility" format="boolean"/>
        <attr name="left_title" format="reference"/>
        <attr name="left_title_left_margin" format="dimension"/>
        <attr name="new_visibility" format="boolean"/>
        <attr name="arrow_visibility" format="boolean"/>
        <attr name="right_title" format="reference"/>
        <attr name="right_title_color" format="reference"/>
        <attr name="root_bg" format="reference"/>

    </declare-styleable>
    <declare-styleable name="KingTitleView">
        <attr name="leftBackImg" format="boolean"/>
        <attr name="titleLeftImg" format="reference"/>
        <attr name="kingTitle" format="reference"/>
        <attr name="rightImg" format="reference"/>
        <attr name="rightText" format="reference"/>
    </declare-styleable>

</resources>